{
  "name": "react-microbe-ui",
  "version": "0.2.0-alpha",
  "description": "microbe-ui for React",
  "scripts": {
    "prebuild": "del-cli ./components",
    "build": "rollup -c",
    "postbuild": "del-cli ./components/blank.d.ts ./components/blank.js",
    "prepare": "npm run build",
    "test": "npm run prettier",
    "fix": "npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{ts,tsx}\" --check"
  },
  "files": [
    "components"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 90,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microbe-ui/react-microbe-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microbe-ui/react-microbe-ui/issues"
  },
  "homepage": "https://github.com/microbe-ui/react-microbe-ui#readme",
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "del-cli": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts-ts": "^4.0.8",
    "react-test-renderer": "^16.13.0",
    "rollup": "^2.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.26.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
